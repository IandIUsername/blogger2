<h1>All Articles</h1>

<ul id="articles">
  <% @articles.each do |article| %>
  <li>
    <%= link_to article.title, article_path(article), class: 'article title', id: "article_#{article.id}" %>
  </li>
  <% end %>
  </ul>
</br>
<%= link_to "create new article", new_article_path, class: "new article" %>         
